			CurrentTrackAlbum$ = ParsePercent(mid(InStatus$,indexStart, Length));
		}
	}
	Else {
		CurrentTrackAlbum$ = "<album n/a>";
	}
	indexStart = find(" url%3A",InStatus$,indexEnd) + 7;
