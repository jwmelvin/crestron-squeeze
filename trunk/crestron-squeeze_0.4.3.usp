			indexEnd = find(" ",AlbumsParseItem, indexStart);
			Length = indexEnd - indexStart;
			AlbumArtist[i] = ParsePercent(mid(AlbumsParseItem,indexStart,Length));
			trace("albumArtist[%u]=%s",i,AlbumArtist[i]);
		}
